    <distribution id="posterior" spec="util.CompoundDistribution" useThreads="false">
        <distribution id="prior" spec="util.CompoundDistribution" useThreads="false">
            <distribution birthDiffRate="@birthRate.alignment" conditionalOnRoot="false" id="YuleModel.alignment" spec="beast.evolution.speciation.YuleModel" tree="@Tree.alignment"/>
            <distribution id="YuleBirthRatePrior.alignment" spec="beast.math.distributions.Prior" x="@birthRate.alignment">
                <distr id="Uniform.0" lower="0.0" offset="0.0" spec="beast.math.distributions.Uniform" upper="1.0"/>
            </distribution>
            <distribution count="@RBScount.alignment" id="RBSprior.alignment" spec="beast.math.distributions.RBPrior" x="@RBSrates.alignment">
                <distr spec="beast.math.distributions.Gamma" alpha='0.2' beta='5.0'/>
            </distribution>
        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="false">
            <distribution data="@alignment" id="treeLikelihood.alignment" spec="TreeLikelihood" tree="@Tree.alignment" useAmbiguities="false">
                <siteModel gammaCategoryCount="0" id="SiteModel.alignment" spec="SiteModel">
                    <parameter dimension="1" estimate="false" id="mutationRate.alignment" minordimension="1" name="mutationRate" value="1.0"/>
                    <parameter dimension="1" estimate="false" id="gammaShape.alignment" minordimension="1" name="shape" value="1.0"/>
                    <parameter dimension="1" estimate="false" id="proportionInvariant.alignment" minordimension="1" name="proportionInvariant" value="0.0"/>
                    <substModel count="@RBScount.alignment" eigenSystem="beast.evolution.substitutionmodel.DefaultEigenSystem" id="RBS.alignment" rates="@RBSrates.alignment" spec="RB">
                        <frequencies data="@alignment" estimate="true" id="freqs.alignment" spec="Frequencies"/>
                    </substModel>
                </siteModel>
                <branchRateModel id="StrictClock.alignment" spec="beast.evolution.branchratemodel.StrictClockModel">
                    <parameter dimension="1" estimate="false" id="clockRate.alignment" minordimension="1" name="clock.rate" value="1.0"/>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>


    
<run chainLength="10000000" id="mcmc" preBurnin="0" sampleFromPrior="false" spec="MCMC" storeEvery="-1">
    <state id="state" storeEvery="1000">
        <tree estimate="true" id="Tree.alignment" name="stateNode">
            <taxonset id="TaxonSet.alignment" spec="TaxonSet">
                <data idref="alignment" name="alignment"/>
            </taxonset>
        </tree>
        <stateNode dimension="1" estimate="true" id="RBScount.alignment" lower="0" minordimension="1" spec="parameter.IntegerParameter" upper="5" value="5"/>
        <parameter dimension="5" estimate="true" id="RBSrates.alignment" lower="0.01" minordimension="1" name="stateNode" upper="100.0" value="1"/>
        <parameter dimension="1" estimate="true" id="birthRate.alignment" minordimension="1" name="stateNode" value="1.0"/>
    </state>

    <distribution idref="posterior"/>

    <operator degreesOfFreedom="1" id="YuleBirthRateScaler.alignment" optimise="true" parameter="@birthRate.alignment" rootOnly="false" scaleAll="false" scaleAllIndependently="false" scaleFactor="0.75" spec="ScaleOperator" weight="3.0"/>

    <operator id="allTipDatesRandomWalker.alignment" spec="TipDatesRandomWalker" tree="@Tree.alignment" useGaussian="false" weight="0.0" windowSize="1.0"/>

    <operator degreesOfFreedom="1" id="treeScaler.alignment" optimise="true" rootOnly="false" scaleAll="false" scaleAllIndependently="false" scaleFactor="0.5" spec="ScaleOperator" tree="@Tree.alignment" weight="1.0"/>

    <operator id="UniformOperator.alignment" spec="Uniform" tree="@Tree.alignment" weight="10.0"/>

    <operator gaussian="true" id="SubtreeSlide.alignment" optimise="true" size="1.0" spec="SubtreeSlide" tree="@Tree.alignment" weight="5.0"/>

    <operator id="narrow.alignment" isNarrow="true" spec="Exchange" tree="@Tree.alignment" weight="1.0"/>

    <operator id="wide.alignment" isNarrow="false" spec="Exchange" tree="@Tree.alignment" weight="1.0"/>

    <operator id="WilsonBalding.alignment" spec="WilsonBalding" tree="@Tree.alignment" weight="1.0"/>

    <operator count="@RBScount.alignment" id="RBSOperator.alignment" rates="@RBSrates.alignment" spec="RBOperator" weight="1.0"/>

    <operator count="@RBScount.alignment" degreesOfFreedom="1" id="RBSrateScaler.alignment" optimise="true" parameter="@RBSrates.alignment" rootOnly="false" scaleAll="false" scaleAllIndependently="false" scaleFactor="0.5" spec="RBScaleOperator" weight="1.0"/>

    <logger fileName="ape.$(seed).log" id="tracelog" logEvery="1000" mode="autodetect" model="@posterior">
        <distribution idref="posterior" name="log"/>
        <distribution idref="likelihood" name="log"/>
        <distribution idref="prior" name="log"/>
        <distribution idref="treeLikelihood.alignment" name="log"/>
        <log id="TreeHeight.alignment" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.alignment"/>
        <distribution idref="YuleModel.alignment" name="log"/>
        <log idref="RBScount.alignment"/>
        <parameter idref="RBSrates.alignment" name="log"/>
        <parameter idref="birthRate.alignment" name="log"/>
    </logger>

    <logger id="screenlog" logEvery="1000" mode="autodetect">
        <distribution idref="posterior" name="log"/>
        <log arg="@posterior" id="ESS.0" spec="util.ESS"/>
        <distribution idref="likelihood" name="log"/>
        <distribution idref="prior" name="log"/>
    </logger>

    <logger fileName="ape.$(seed).trees" id="treelog.alignment" logEvery="1000" mode="tree">
        <tree idref="Tree.alignment" name="log"/>
    </logger>

    <tree estimate="false" id="RandomTree.alignment" initial="@Tree.alignment" name="init" spec="beast.evolution.tree.RandomTree" taxa="@alignment">
        <populationModel id="ConstantPopulation0.alignment" spec="ConstantPopulation">
            <parameter dimension="1" estimate="true" id="randomPopSize.alignment" minordimension="1" name="popSize" value="1"/>
        </populationModel>
    </tree>

    <operatorschedule autoOptimize="true" autoOptimizeDelay="10000" id="OperatorSchedule.0" spec="OperatorSchedule" transform="none"/>

</run>



